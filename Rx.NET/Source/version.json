  "version": "6.1",
  "publicReleaseRefSpec": [
    "^refs/heads/main$", // we release out of main
    "^refs/heads/rel/v\\d+\\.\\d+", // we also release branches starting with rel/vN.N
    "^refs/heads/rel/rx-v\\d+\\.\\d+" // we also release branches starting with rel/vN.N
